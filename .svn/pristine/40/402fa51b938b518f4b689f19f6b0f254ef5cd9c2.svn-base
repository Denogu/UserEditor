<!--- Used on multiple forms --->
<cfoutput>
<div class="form-group has-feedback">
<label for="subject_number_field" class="control-label">Participant ID Number</label>
<input type="text" 
  class="form-control" 
  name="subject_number" 
  placeholder="Site Number followed by 4 digits"
  id="subject_number_field" 
  title="Participant ID Number" 
  autofocus 
  required 
  pattern="^#session.user.site_number#\d{4}$"/>
<span id="subject_number_error_field" class="hidden text-danger">Must be 3-digit site number followed by 4 digits.</span>
</div>
</cfoutput>
